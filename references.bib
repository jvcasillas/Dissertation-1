	Journal = {PloS one},
	Keywords = {Adult; Alpha Rhythm/*physiology; Brain/*physiology; Female; Humans; Male; *Periodicity; *Photic Stimulation; Visual Perception/*physiology; Young Adult},
	L2 = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3612058/},
	La = {eng},
	Number = {3},
	Pages = {e60035--e60035},
	Publisher = {Public Library of Science},
	Title = {Alpha-band rhythms in visual task performance: phase-locking by rhythmic sensory stimulation},
	Ty = {JOUR},
